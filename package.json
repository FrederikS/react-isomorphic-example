{
  "name": "react-isomorphic",
  "version": "0.0.1",
  "description": "Example of isomorphic JS with React",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:server": "node_modules/.bin/webpack --config config/webpack.config.server-watch.js",
    "start:api": "npm run build:api && node external/server.api.build.js",
    "start": "npm run lint && npm run server:watch",
    "build:client": "node_modules/.bin/webpack --watch --config config/webpack.config.client.js",
    "build:server": "node_modules/.bin/webpack --config config/webpack.config.server.js",
    "build:api": "node_modules/.bin/webpack --config config/webpack.config.server.api.js",
    "server:watch": "node_modules/.bin/nodemon --watch server.js --watch src --exec npm run start:server",
    "lint": "node_modules/.bin/eslint src"
  },
  "keywords": [
    "isomorphic",
    "react"
  ],
  "author": "frederik.steffen",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-decorators": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "isomorphic-style-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.9.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2",
    "redux-promise-middleware": "^3.3.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-node-externals": "^1.2.0",
    "webpack-shell-plugin": "^0.4.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.14.1"
  }
}
